.container{
  margin: 0;
  padding-top:20rpx; 
}

.inputView image{
    position: absolute;
    right: 40%;
    top: 30rpx;
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
}
/*表单内容*/
.inputView {
    background-color: white;
    line-height: 45px;
}
.line {
    width: 100%;
    height: 1px;
    background-color: gainsboro;
    margin-top: 1px;
}
.inputText {
    float: right;
    text-align: right;
    margin-right: 22px;
    margin-top: 11px;
    font-size: 15px;
}
.formLabel {
    margin: 15px 15px 15px 10px;
    color: gray;
    font-size: 15px;
}
.name{
  background-color: #da9b5d;
}
/*按钮*/
.changeBtn {
    width: 80%;
    margin-top: 35px;
}

.goBackBtn {
    width: 80%;
    margin-top: 15px;
}